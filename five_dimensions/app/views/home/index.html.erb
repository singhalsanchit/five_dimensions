                    <div class="slide-item slide1"></div>
                    <div class="slide-item slide2"></div>
                    <div class="slide-item slide3"></div>
                    <div class="slide-item slide4"></div>
                    <div class="slide-item slide5"></div>
                </div>
                <div class="logoOnBanner">
