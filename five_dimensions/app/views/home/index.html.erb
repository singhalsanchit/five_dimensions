        </main>
        <footer>
            <div class="footer-top">
            <div class="container">
              <div class="sidebar-column col-md-6 col-sm-6">
                  <aside id="sydney_contact_info-2" class="widget sydney_contact_info_widget">
                      <h6 class="footer-title">
                          Corporate Office</a>
                      </h6>
                      <div class="contact-address">
                          <span>
                              <i class="fa fa-map-marker"></i>
                          </span>
                          A-7 Paryavaran Complex, IGNOU Road, New Delhi-110030
                      </div>
                      <div class="contact-email">
                          <span>
                              <i class="fa fa-envelope-o"></i>
                          </span>
                          <a href="mailto:info@fourthwallevents.com">events@5dimensions.co.in</a>
                      </div>
                      <div class="contact-phone">
                          <span>
                              <i class="fa fa-phone"></i>
                          </span>
                          8587860180/8826060971
                      </div>
                  </aside>
              </div>
              <div class="sidebar-column col-md-6 col-sm-6">
                  <div class="">
                    <h6 class="footer-title">Write Us</h6>
                    <div class="row">
                      <%= form_tag("/send_request", method: "post",class: "form-horizontal clearfix") do %>
                      <div class="col-md-6">
                      <div class="form-group">
                      <div class="col-md-12 col-sm-12">
                          <%= label_tag(:name, "Your Name") %>
                        <%= text_field_tag :name, nil,:placeholder => "Your Name", :class => "form-control text-fields",:required => true %>
                        </div>
                        </div>   

                      </div>
                      <div class="col-md-6">
                      <div class="form-group">
                      <div class="col-md-12 col-sm-12">
                        <%= label_tag(:email, "Your Email") %>
                        <%= email_field_tag :email, nil,:placeholder => "Your Email", :class => "form-control text-fields",:required => true %>
                        </div>
                        </div>
                      </div>
                      <div class="col-md-12">
                      <div class="form-group">
                      <div class="col-md-12 col-sm-12">
                      <%= label_tag(:message, "Message") %>
                        <%= text_area_tag :message, nil,:placeholder => "Hi, please write your message here!", size: "24x6", :class => "form-control text-fields",:required => true %>
                        </div>
                        </div>
                      </div>
                      <div class="col-md-12">
                      <div class="form-group">
                      <div class="col-md-12 col-sm-12">
                        <%= submit_tag("Send Request", class: "btn btn-default submit-btn submit-btn-mgbt30") %>
                        </div>
                        </div>
                      </div>
                      <% end %>
                    </div>
              </div>
              </div>
            </div>
            <div class="copy-right">
                <div class="site-info container">
                    © 2018 Five Dimensions Events 
                </div>
            </div>
        </footer>
        <script>
            $(document).ready(function () {
                $(window).resize(function () {
                    var bodyheight = $(this).height();
                    $(".herosection").height(bodyheight);
                }).resize();
            });
            $('.herosection-slider').slick({
                fade: true,
                slidesToShow: 1,
                slidesToScroll: 1,
                arrows: false,
                autoplay: true,
                autoplaySpeed: 2000
            });
            $('.client-slide').slick({
                infinite: true,
                slidesToShow: 5,
                slidesToScroll: 5,
                arrows: false,
                dots: true
            });
            $('.testimonial_slider').slick({
                infinite: true,
                slidesToShow: 2,
                slidesToScroll: 2,
                arrows: false,
                dots: true,
                autoplay:true
            });
        </script> 
